{
  "name": "schedule-me-up",
  "version": "0.1.0",
  "scripts": {
    "dev": "nodemon server.js",
    "test": "npm run test:model && npm run test:routes",
    "test:model": "npm run db:init && jest tests/model/ -i",
    "test:routes": "npm run db:init && jest tests/routes/ -i",
    "db:init": "node db/util/initDatabase.js",
    "precommit": "lint-staged",
    "lint": "eslint --fix ."
  },
  "description": "",
  "main": "server.js",
  "author": "Group 2",
  "lint-staged": {
    "**/*.{js,jsx,scss}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "dependencies": {
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.16.2",
    "express-validator": "^6.4.0",
    "jest": "^25.1.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "passport": "^0.4.1",
    "passport-google-token": "^0.1.2",
    "promise-mysql": "^4.1.2",
    "require-sql": "^1.0.0",
    "swagger-ui-express": "^4.1.3"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-json": "^2.1.0",
    "eslint-plugin-prettier": "^3.1.2",
    "lint-staged": "^10.0.8",
    "nodemon": "^2.0.2",
    "passport-mock-strategy": "^2.0.0",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2"
  }
}
