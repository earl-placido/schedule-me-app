{
  "name": "ScheduleMeUp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "test:ci": "set CI=true && jest",
    "test:system": "node launchSystemTests",
    "lint": "eslint --fix src",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "**/*.{js,jsx,json,scss}": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.8.1",
    "@react-native-community/google-signin": "^3.0.4",
    "@react-native-community/masked-view": "^0.1.7",
    "@react-navigation/drawer": "^5.2.0",
    "@react-navigation/native": "^5.0.9",
    "@react-navigation/stack": "^5.1.1",
    "axios": "^0.19.2",
    "moment": "^2.24.0",
    "native-base": "^2.13.8",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2",
    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-native-calendars": "^1.264.0",
    "react-native-datepicker": "^1.7.2",
    "react-native-dialog": "^5.6.0",
    "react-native-elements": "^1.2.7",
    "react-native-floating-action": "^1.20.0",
    "react-native-gesture-handler": "^1.6.0",
    "react-native-loading-spinner-overlay": "^1.1.0",
    "react-native-modal": "^11.5.4",
    "react-native-progress-steps": "^1.2.8",
    "react-native-reanimated": "^1.7.0",
    "react-native-safe-area-context": "^0.7.3",
    "react-native-screens": "^2.2.0",
    "react-redux": "^7.2.0",
    "react-router-native": "^5.1.2",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "tcomb-form-native": "^0.6.20"
  },
  "devDependencies": {
    "@babel/core": "7.8.4",
    "@babel/runtime": "7.8.4",
    "@react-native-community/eslint-config": "0.0.5",
    "@wdio/appium-service": "6.0.0",
    "@wdio/cli": "6.0.0",
    "@wdio/jasmine-framework": "6.0.0",
    "@wdio/local-runner": "6.0.0",
    "@wdio/sync": "6.0.0",
    "babel-jest": "24.9.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-json": "^2.1.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-wdio": "^6.0.12",
    "husky": "^4.2.3",
    "jest": "24.9.0",
    "lint-staged": "^10.0.8",
    "metro-react-native-babel-preset": "0.56.4",
    "react-dom": "^16.13.1",
    "react-native-config": "^0.12.0",
    "react-test-renderer": "16.9.0",
    "redux-mock-store": "^1.5.4",
    "typescript": "^3.8.3"
  },
  "jest": {
    "preset": "react-native",
    "testRegex": "./__tests__/.*.js$",
    "transform": {
      "^.+\\.(js)$": "./node_modules/react-native/jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!native-base)/"
    ],
    "setupFiles": [
      "./node_modules/react-native-gesture-handler/jestSetup.js",
      "./__mocks__/@react-native-community/google-signin.js",
      "./__mocks__/@react-native-community/async-storage.js",
      "./__mocks__/redux-persist/persistgate.js",
      "./__mocks__/react-native-reanimated/setup.js"
    ]
  }
}
