{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.0.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "antd": "^4.0.3",
    "axios": "^0.19.2",
    "axios-mock-adapter": "^1.17.0",
    "jwt-decode": "^2.2.0",
    "node-sass": "^4.13.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.12.0",
    "react-google-login": "^5.1.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "start-server-and-test": "^1.10.10"
  },
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "start": "react-scripts start",
    "build_prod": "NODE_ENV=production react-scripts build",
    "build_dev": "NODE_ENV=development react-scripts build",
    "test": "jest --config ./jest.config.json",
    "test:coverage": "jest --config ./jest.config.json --coverage",
    "test:ci": "CI=true react-scripts test",
    "test:integration:open": "./node_modules/.bin/cypress open --config-file cypress.integration.json",
    "test:integration:run": "./node_modules/.bin/cypress run --config-file cypress.integration.json",
    "test:acceptance:open": "./node_modules/.bin/cypress open --config-file cypress.acceptance.json",
    "test:acceptance:run": "./node_modules/.bin/cypress run --config-file cypress.acceptance.json",
    "test:ui": "npm run test:integration:run && npm run test:acceptance:run",
    "test:ui:ci": "start-server-and-test start http://localhost:3000 test:ui",
    "eject": "react-scripts eject",
    "deploy_dev": "surge --domain schedule-me-up-dev.surge.sh build",
    "scss": "node-sass --watch src/scss -o src/css",
    "precommit": "lint-staged",
    "lint": "eslint --fix ."
  },
  "resolutions": {
    "terser": "4.6.7"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "lint-staged": {
    "**/*.{js,jsx,json,scss}": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "cypress": "^4.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-json": "^2.1.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "husky": "^4.2.1",
    "jest": "^24.9.0",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.12.0",
    "redux-mock-store": "^1.5.4"
  }
}
